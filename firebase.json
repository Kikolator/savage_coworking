{
  "firestore": {
    "database": "(default)",
    "location": "eur3",
    "rules": "shared/database/firestore.rules",
    "indexes": "shared/database/firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "disallowLegacyRuntimeConfig": true,
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint",
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    }
  ],
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "rules": "shared/database/storage.rules"
  },
  "emulators": {
    "auth": {
      "port": 9095
    },
    "functions": {
      "port": 5005
    },
    "firestore": {
      "port": 8081
    },
    "hosting": {
      "port": 5010
    },
    "storage": {
      "port": 9198
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  },
  "flutter": {
    "platforms": {
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "savage-coworking",
          "configurations": {
            "web": "1:489719466429:web:b0f56ec9bd67224764aad4"
          }
        }
      }
    }
  }
}
